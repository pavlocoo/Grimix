function pages:openpage()if self.open==false then for i,v in pairs(self.library.pages)do if v~=self then if v.open then v.page.Visible=false v.open=false v.outline.BackgroundColor3=Color3.fromRGB(24,24,24)v.line.Size=UDim2.new(1,0,0,2)v.line.BackgroundColor3=Color3.fromRGB(24,24,24)end end end self.page.Visible=true self.open=true self.outline.BackgroundColor3=Color3.fromRGB(20,20,20)self.line.Size=UDim2.new(1,0,0,3)self.line.BackgroundColor3=Color3.fromRGB(20,20,20)end end

function watermarks:update(c)local t=""for i,v in pairs(c)do t=t..i..": "..v.."  "end t=t:sub(0,-3)self.title.Text=t end
function watermarks:updateside(s)s=utility.removespaces(tostring(s):lower())local si={topright={AnchorPoint=Vector2.new(1,0),Position=UDim2.new(1,-10,0,10)},topleft={AnchorPoint=Vector2.new(0,0),Position=UDim2.new(0,10,0,10)},bottomright={AnchorPoint=Vector2.new(1,1),Position=UDim2.new(1,-10,1,-10)},bottomleft={AnchorPoint=Vector2.new(0,1),Position=UDim2.new(0,10,1,-10)}}if si[s]then self.outline.AnchorPoint=si[s].AnchorPoint self.outline.Position=si[s].Position end end

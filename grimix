function loaders:toggle()self.outline.Visible=true end
function watermarks:toggle(b)self.outline.Visible=b end
function library:saveconfig()local c={}for i,v in pairs(self.pointers)do c[i]={}for d,p in pairs(v)do c[i][d]={}for x,z in pairs(p)do if typeof(z.current)=="Color3"then c[i][d][x]={z.current.R,z.current.G,z.current.B}else c[i][d][x]=z.current end end end end return hs:JSONEncode(c)end
function library:loadconfig(c)local c=hs:JSONDecode(readfile(c))for i,v in pairs(c)do for d,p in pairs(v)do for x,z in pairs(d)do if z~=nil then if self.pointers[i]~=nil and self.pointers[i][d]~=nil and self.pointers[i][d][x]~=nil then self.pointers[i][d][x]:set(z)end end end end end end
function library:settheme(t,c)if self.theme[t]then self.theme[t]=c end if self.themeitems[t]then for i,v in pairs(self.themeitems[t])do for z,x in pairs(v)do x[i]=c end end end end
function library:setkey(k)if typeof(k)=="EnumItem"then self.key=k end end
function library:settoggle(s,b)if s=="x"then self.x=b else self.y=b end end
function library:setfont(f)if f~=nil then for i,v in pairs(self.labels)do if v~=nil then v.Font=f end end end end
function library:settextsize(s)if s~=nil then for i,v in pairs(self.labels)do if v~=nil then v.TextSize=s end end end end
